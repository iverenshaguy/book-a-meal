{
  "name": "server",
  "version": "2.0.0",
  "main": "src/app.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "12.18.3"
  },
  "scripts": {
    "build": "babel --root-mode upward ./src -d ./dist",
    "pretest": "npm run db:setup:test",
    "db:migrate": "npm run sequelize-cli:es6 db:migrate",
    "db:migrate:undo": "npm run sequelize-cli:es6 db:migrate:undo:all",
    "db:seed": "npm run sequelize-cli:es6 db:seed:all",
    "db:setup": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
    "db:setup:test": "NODE_ENV=test npm run db:setup",
    "test": "NODE_ENV=test mocha --require tests/config ./tests/test.js --timeout 50000 --exit",
    "test:watch": "npm run test --watch",
    "test:coverage": "nyc --reporter=lcov --reporter=text npm run test",
    "sequelize-cli:es6": "babel-node --root-mode upward ../../node_modules/.bin/sequelize $*",
    "prebuild:v2": "rimraf ./v2/dist",
    "build:v2": "nest build",
    "format:v2": "prettier --write \"v2/src/**/*.ts\" \"v2/tests/**/*.ts\"",
    "start:v2": "nest start",
    "start:dev:v2": "nest start --watch",
    "start:debug:v2": "nest start --debug --watch",
    "start:prod:v2": "node v2/dist/main",
    "lint:v2": "eslint --config ./eslintrc.v2.js  \"{v2/src,apps,libs,test}/**/*.ts\" --fix",
    "test:v2": "jest",
    "test:watch:v2": "jest --watch",
    "test:cov:v2": "jest --coverage",
    "test:debug:v2": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:v2": "jest --config ./v2/tests/jest-e2e.json"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/sequelize": "^0.1.0",
    "@types/passport-jwt": "^3.0.3",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.3",
    "connect-gzip-static": "^2.1.1",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-validator": "^5.1.2",
    "helmet": "^3.20.0",
    "jsonwebtoken": "^8.2.1",
    "mockdate": "^2.0.2",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.4",
    "parse-database-url": "^0.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "7.4.1",
    "pg-hstore": "^2.3.2",
    "random-string": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sequelize": "^6.3.4",
    "sequelize-typescript": "^2.0.0-beta.0",
    "uuid": "^3.2.1",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/node": "^13.9.1",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "istanbul": "^0.4.5",
    "jest": "26.0.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "react-dom": "^16.8.6",
    "sequelize-cli": "^5.5.0",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
