{
  "name": "server",
  "version": "2.0.0",
  "main": "src/app.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "12.18.3"
  },
  "scripts": {
    "build": "babel --root-mode upward ./src -d ./dist",
    "pretest": "npm run db:setup:test",
    "db:migrate": "npm run sequelize-cli:es6 db:migrate",
    "db:migrate:undo": "npm run sequelize-cli:es6 db:migrate:undo:all",
    "db:seed": "npm run sequelize-cli:es6 db:seed:all",
    "db:setup": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
    "db:setup:test": "NODE_ENV=test npm run db:setup",
    "test": "NODE_ENV=test mocha --require tests/config ./tests/test.js --timeout 50000 --exit",
    "test:watch": "npm run test --watch",
    "test:coverage": "nyc --reporter=lcov --reporter=text npm run test",
    "sequelize-cli:es6": "babel-node --root-mode upward ../../node_modules/.bin/sequelize $*"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.3",
    "connect-gzip-static": "^2.1.1",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-validator": "^5.1.2",
    "helmet": "^3.20.0",
    "jsonwebtoken": "^8.2.1",
    "mockdate": "^2.0.2",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.4",
    "pg": "7.4.1",
    "pg-hstore": "^2.3.2",
    "random-string": "^0.2.0",
    "sequelize": "^5.15.1",
    "uuid": "^3.2.1",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "react-dom": "^16.8.6",
    "sequelize-cli": "^5.5.0",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
