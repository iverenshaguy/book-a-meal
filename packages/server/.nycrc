{
  "check-coverage": true,
  "extends": "@istanbuljs/nyc-config-babel",
  "per-file": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches":80,
  "require": ["@babel/register"],
  "exclude": [
    "**/*.test.js",
    "**/*.spec.js",
    "**/__tests__/**",
    "**/node_modules/**",
    "config",
    "coverage/**",
    "tests/**",
    "src/models/index.js",
    "src/sequelize.js",
    "src/v2/**",
    "test{,-*}.js",
    "config{,-*}.js",
    "docs.**"
  ],
  "reporter": ["lcov", "json", "text"],
  "sourceMap": false,
  "instrument": false,
  "cache": true,
  "report-dir": "./coverage"
}