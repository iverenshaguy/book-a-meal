{
  "check-coverage": true,
  "extends": "@istanbuljs/nyc-config-babel",
  "per-file": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches":80,
  "require": ["@babel/register"],
  "exclude": [
    "**/*.test.js",
    "**/*.spec.js",
    "**/__tests__/**",
    "**/node_modules/**",
    "client/",
    "server/config",
    "server/coverage/**",
    "server/tests/**",
    "server/src/models/index.js",
    "server/src/sequelize.js",
    "server/test{,-*}.js",
    "server/docs.**",
    "webpack.**.js"
  ],
  "reporter": ["lcov", "json", "text"],
  "sourceMap": false,
  "instrument": false,
  "cache": true,
  "report-dir": "./coverage"
}