  "scripts": {
    "start": "node ./server/dist/app.js",
    "start:dev": "nodemon --exec babel-node -- ./server/src/app.js",
    "start:test": "NODE_ENV=test babel-node -- ./server/src/app.js",
    "build": "./scripts/build.sh",
    "build:server": "babel ./server/src -d ./server/dist",
    "build:client": "./client/scripts/build.sh",
    "pretest": "npm run lint",
    "pretest:server": "npm run db:setup:test",
    "pretest:e2e": "npm run pretest:server",
    "lint": "eslint '**/*.{js,jsx}' --fix",
    "db:migrate": "npm run sequelize-cli:es6 db:migrate",
    "db:migrate:undo": "npm run sequelize-cli:es6 db:migrate:undo:all",
    "db:seed": "npm run sequelize-cli:es6 db:seed:all",
    "db:setup": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
    "db:setup:test": "NODE_ENV=test npm run db:setup",
    "coverage": "npm run merge:lcov && cat ./coverage/lcov.info | coveralls",
    "test": "npm-run-all test:server test:client",
    "test:coverage": "npm-run-all test:server:coverage test:client:coverage",
    "test:server": "NODE_ENV=test mocha --require @babel/register ./server/tests/test.js --timeout 50000 --exit",
    "test:server:watch": "npm run test:server --watch",
    "test:server:coverage": "nyc --reporter=lcov --reporter=text npm run test:server",
    "test:client": "NODE_ENV=jsdom jest --env=jsdom",
    "test:client:watch": "npm run test:client --watch",
    "test:client:coverage": "NODE_ENV=jsdom jest --env=jsdom --coverage",
    "sequelize-cli:es6": "babel-node ./node_modules/.bin/sequelize $*",
    "merge:lcov": "cat ./coverage/lcov.*.info > ./coverage/lcov.info",
    "e2e:setup": "selenium-standalone install --config=./selenium.config.js",
    "test:e2e": "NODE_ENV=test nightwatch"
  },
