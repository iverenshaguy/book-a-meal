  "scripts": {
    "build": "./scripts/build.sh",
    "coverage": "run-s create:coverage:folder sed:lcov merge:lcov push:coverage:coveralls",
    "lint": "eslint '**/*.{js,jsx}' --fix",
    "pretest": "yarn lint",
    "start": "node ./dist/server.js",
    "start:dev": "nodemon --exec babel-node --root-mode upward ./src/server.js",
    "test": "NODE_ENV=jsdom jest --env=jsdom",
    "test:watch": "npm run test --watch",
    "test:coverage": "run-s test:cov:v1",
    "test:cov:v1": "NODE_ENV=jsdom jest --env=jsdom --coverage",
    "test:cov:v2": "NODE_ENV=jsdom jest --env=jsdom --coverage",
    "create:coverage:folder": "rm -rf ./coverage && mkdir ./coverage",
    "merge:lcov": "cat ./coverage/lcov-v1.info ./coverage/lcov-v2.info > ./coverage/lcov.info",
    "push:coverage:coveralls": "cat ./coverage/lcov.info | coveralls",
    "sed:lcov": "run-s sed:lcov:v1 sed:lcov:v2",
    "sed:lcov:v1": "sed 's/SF:/SF:\\//g' ./coverage/lcov.info > ./coverage/lcov-v1.info",
    "sed:lcov:v2": "sed 's/SF:/SF:\\//g' ./v2/coverage/lcov.info > ./coverage/lcov-v2.info"
  },
